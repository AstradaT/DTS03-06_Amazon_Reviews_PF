{
	"name": "CopyPipeline_bqi",
	"properties": {
		"activities": [
			{
				"name": "Copy_bqi",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "datos//Books_5.json"
					},
					{
						"name": "Destination",
						"value": "datos//pruebadatafactory"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "setOfObjects"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['reviewerID']"
								},
								"sink": {
									"path": "$['reviewerID']"
								}
							},
							{
								"source": {
									"path": "$['asin']"
								},
								"sink": {
									"path": "$['asin']"
								}
							},
							{
								"source": {
									"path": "$['reviewerName']"
								},
								"sink": {
									"path": "$['reviewerName']"
								}
							},
							{
								"source": {
									"path": "$['helpful'][0]"
								},
								"sink": {
									"path": "$['helpful']"
								}
							},
							{
								"source": {
									"path": "$['reviewText']"
								},
								"sink": {
									"path": "$['reviewText']"
								}
							},
							{
								"source": {
									"path": "$['overall']"
								},
								"sink": {
									"path": "$['overall']"
								}
							},
							{
								"source": {
									"path": "$['summary']"
								},
								"sink": {
									"path": "$['summary']"
								}
							},
							{
								"source": {
									"path": "$['unixReviewTime']"
								},
								"sink": {
									"path": "$['unixReviewTime']"
								}
							},
							{
								"source": {
									"path": "$['reviewTime']"
								},
								"sink": {
									"path": "$['reviewTime']"
								}
							}
						],
						"collectionReference": "$['helpful']"
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_bqi",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_bqi",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}